{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "lint": {},
    "check-types": {},
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "deploy": {
      "dependsOn": ["lint", "build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
