fn req_url():
  return bs4.htmlparse()


fn iter_pages(initial_url: str, selector: str) -> int:
  url = None
  iter_page_count = 0

  while True:
    page = req_url(url)
    iter_page_count += 1

    _selector = page.select(selector)

    if !_selector:
      return iter_page_count
      break

fn main():
  fa_url = "furaffinity.net"

  if !cache:
    fa_page_count = iter_pages(fa_url, selector)
  